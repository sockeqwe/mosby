<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hannesdorfmann.mosby.sample">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/AppTheme"
      android:name=".SampleApplication">

    <activity
        android:name=".MainActivity"
        android:label="@string/title_activity_main">

      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>

    </activity>

    <activity android:name=".FragmentContainerActivity"
        android:label="FragmentActivity"/>

    <activity
        android:name=".mvp.lce.activity.CountriesActivity"
        android:label="@string/title_activity_lce">
    </activity>

    <activity android:name=".mvp.lce.viewstate.NotRetainingCountriesActivity"
        android:label="NotRetainingCountriesActivity"
        />

    <activity android:name=".mvp.lce.viewstate.RetainingCountriesActivity"
      android:label="RetainingCountriesActivity" />

    <activity android:name=".mvp.customviewstate.MyCustomActivity"
        android:label="Custom ViewState Activity"/>

    <activity android:name=".mvp.lce.layout.CountriesLayoutActivity"
        android:label="MVP Framelayout"/>

    <activity android:name=".mvp.lce.viewstate.RetainingCountriesFragmentEmbededInXmlActivity"
      android:label="Embeded ViewState Fragment"
      />
    
    <activity android:name=".mvp.lce.viewpager.ViewPagerActivity"
      android:label="ViewPager" />

    <activity android:name=".mvp.sce.activity.CountryFormActivity"
      android:label="SCE Activity" />

  </application>


</manifest>
